{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap/bootstrap-icons/"
    },
    {
      "library": "bootstrap@5.2.3",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/toastr.js/",
      "files": [
        "toastr.min.css"
      ]
    }
  ]
}